#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/Anton/Documents/backend/SPINT1/week03/hw03_2/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@_47d73c87f613df164d566788699f161e/node_modules/ts-jest/node_modules:/proc/cygdrive/c/Users/Anton/Documents/backend/SPINT1/week03/hw03_2/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@_47d73c87f613df164d566788699f161e/node_modules:/proc/cygdrive/c/Users/Anton/Documents/backend/SPINT1/week03/hw03_2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/Anton/Documents/backend/SPINT1/week03/hw03_2/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@_47d73c87f613df164d566788699f161e/node_modules/ts-jest/node_modules:/proc/cygdrive/c/Users/Anton/Documents/backend/SPINT1/week03/hw03_2/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@_47d73c87f613df164d566788699f161e/node_modules:/proc/cygdrive/c/Users/Anton/Documents/backend/SPINT1/week03/hw03_2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ts-jest@29.1.1_@babel+core@_47d73c87f613df164d566788699f161e/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/ts-jest@29.1.1_@babel+core@_47d73c87f613df164d566788699f161e/node_modules/ts-jest/cli.js" "$@"
fi
